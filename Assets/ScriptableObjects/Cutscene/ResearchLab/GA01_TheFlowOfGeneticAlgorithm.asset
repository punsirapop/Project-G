        Speaker: 0
        SentenceContent: 
        Illustration: {fileID: 0}
  - IsChoices: 0
    IsChecker: 0
    SentenceData:
      Speaker: 10
      SentenceContent: But using this algorithm, <b>optimal solutions</b> will be
        found.
      Illustration: {fileID: 21300000, guid: 1af671848ced23f4294888e4cc3108fa, type: 3}
    Choices: []
    CheckerAnswer:
      Pass:
        Speaker: 0
        SentenceContent: 
        Illustration: {fileID: 0}
