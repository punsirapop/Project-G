        Speaker: 0
        SentenceContent: 
        Illustration: {fileID: 0}
  - IsChoices: 0
    IsChecker: 0
    SentenceData:
      Speaker: 3
      SentenceContent: It is about making things better.
      Illustration: {fileID: 21300000, guid: 1af671848ced23f4294888e4cc3108fa, type: 3}
    Choices: []
    CheckerAnswer:
      Pass:
        Speaker: 0
        SentenceContent: 
        Illustration: {fileID: 0}
      Fail:
        Speaker: 0
        SentenceContent: 
        Illustration: {fileID: 0}
  - IsChoices: 0
    IsChecker: 0
    SentenceData:
      Speaker: 4
      SentenceContent: No right or wrong, the bad answer also be an answer.
      Illustration: {fileID: 21300000, guid: 1af671848ced23f4294888e4cc3108fa, type: 3}
    Choices: []
    CheckerAnswer:
      Pass:
        Speaker: 0
        SentenceContent: 
