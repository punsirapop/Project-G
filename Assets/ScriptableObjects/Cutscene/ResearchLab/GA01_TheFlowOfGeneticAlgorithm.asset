        Illustration: {fileID: 0}
  - IsChoices: 0
    IsChecker: 0
    SentenceData:
      Speaker: 2
      SentenceContent: For clarity, the algorithm is used widely in an <b>optimization</b>
        problems.
      Illustration: {fileID: 21300000, guid: 1af671848ced23f4294888e4cc3108fa, type: 3}
    Choices: []
    CheckerAnswer:
      Pass:
        Speaker: 0
        SentenceContent: 
